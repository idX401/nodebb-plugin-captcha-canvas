{
  "id": "nodebb-plugin-captcha-canvas",
  "name": "NodeBB Ban Privileges",
  "description": "Give account ban privilieges to a group.",
  "url": "https://github.com/idX401/nodebb-plugin-captcha-canvas",
  "library": "./src/library.js",
  "hooks": [
    { "hook": "static:app.load", "method": "init" },
    { "hook": "filter:admin.header.build", "method": "addAdminNavigation" },
    { "hook": "filter:register.build", "method": "addCaptchaRegister" },
    { "hook": "filter:register.check", "method": "checkCaptchaRegister" },
    { "hook": "filter:auth.build", "method": "addCaptchaLogin" },
    { "hook": "filter:login.check", "method": "checkCaptchaLogin" },
    { "hook": "action:nodebb-plugin-prometheus.init", "method": "initPrometheus" }
  ],
  "scripts": [
    "./public/js/main.js"
  ],
  "languages": "./languages",
  "defaultLang": "en-GB",
  "templates": "./public/templates"
}
